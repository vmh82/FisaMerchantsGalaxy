package com.fisa.galaxy.enums;

public enum RomanosEnum {
	/** The I. */
	I(1), 
	
	/** The V. */
	V(5),
	
	/** The X. */
	X(10),
	
	/** The L. */
	L(50),
	
	/** The C. */
	C(100),
	
	/** The D. */
	D(500),
	
	/** The M. */
	M(1000);
	
	/** The value. */
	private int value;
	
	/**
	 * Instantiates a new roman numbers enum.
	 *
	 * @param value the value
	 */
	RomanosEnum(int value){
		this.value = value;
	}
	
	/**
	 * Gets the value.
	 *
	 * @return the value
	 */
	public int getValue(){
		return this.value;
	}
}
